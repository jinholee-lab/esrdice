{
    // VS Code debug setting
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python Debugger: Train Fair Taxi",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/main.py",
            "console": "integratedTerminal",
            "justMyCode": true,
            "env": {
                "CUDA_VISIBLE_DEVICES": "0"
            },
            "args": [
                "--seed", "42",
                "--device", "cuda",
                "--num_steps", "100000",
                "--log_interval", "1000",
                "--batch_size", "256",
                "--horizon", "40",
                "--fuel", "40",
                "--dataset_path", "./data/bus.npy",
                "--reward_dim", "2",
                "--f_divergence", "Chi",
                "--hidden_dims", "256", "256", "256",
                "--alpha", "0.0001",
                "--nu_lr", "3e-4",
                "--policy_lr", "3e-4",
                "--mu_lr", "3e-4",
                "--nu_grad_penalty_coeff", "0.0",
                "--normalization_method", "linear",
                "--num_eval_episodes", "100",
                "--utility_kind", "piecewise_log",
                // "--concat_acc_reward", "True",
                "--one_hot_xy", "True",
                "--one_hot_pass_idx", "True",
                "--mode", "ser",
                "--policy_rollout", "stochastic",
                "--tag", "FairDICE_test"
            ]
        }
    ]
}

