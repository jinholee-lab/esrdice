{
    // VS Code debug setting
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python Debugger: Train Fair Taxi",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/main.py",
            "console": "integratedTerminal",
            "justMyCode": true,
            "args": [
                "--seed", "42",
                "--device", "cuda",
                "--num_steps", "50000",
                "--log_interval", "1000",
                "--batch_size", "256",
                "--dataset_path", "./data/dataset_v3.npy",
                "--f_divergence", "Chi",
                "--alpha", "0.1",
                "--nu_lr", "3e-4",
                "--policy_lr", "3e-4",
                "--nu_grad_penalty_coeff", "0.001",
                "--tag", "esrdice",
                "--use_wandb", "True"
            ]
        }
    ]
}
